  "keywords": [
    "react",
    "token",
    "tag",
    "token input",
    "tag input",
    "preprocess",
    "inline edit",
    "customize",
    "customize data structure",
    "customize render"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.3.2",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
